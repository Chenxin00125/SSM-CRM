<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cx.workbench.dao.TranDao">
    <!--转换创建的交易-->
    <insert id="tranDeal">
        insert into tbl_tran (id,owner,name,money,expectedDate,customerId,stage,source,activityId,contactsId,description,createBy,createTime)
        values(#{id},#{owner},#{name},#{money},#{expectedDate},#{customerId},#{stage},#{source},#{activityId},#{contactsId},#{description},#{createBy},#{createTime})
    </insert>
	
  	
</mapper>

































